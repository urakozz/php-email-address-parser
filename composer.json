{
  "name": "kozz/email-address-parser",
  "type": "library",
  "description": "Simply parses and autocompletes email addresses",
  "keywords": ["email", "parser", "address"],
  "homepage": "http://github.com/urakozz/php-email-address-parser",
  "license": "MIT",

  "authors": [
    {
      "name": "Yury Kozyrev",
      "email": "urakozz@gmail.com",
      "homepage": "https://github.com/urakozz"
    }
  ],

  "require": {
    "php": ">=5.4.0",
    "kozz/collection": "*"
  },

  "suggest": {
    "php": ">=5.5.15"
  },

  "autoload": {
    "psr-4": {
      "Kozz\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kozz\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "satooshi/php-coveralls": "dev-master"
  },

  "minimum-stability": "stable"
}